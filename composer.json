    "type": "Project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Li",
            "email": "ivan.li@invigorgroup.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/cache": "~5.0",
        "laravelcollective/html": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "illuminate/database": "~5.0",
        "sami/sami": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "psr-4": {
            "Invigor\\UM\\": "src/UM/"
        }
    },
    "autoload-dev": {
